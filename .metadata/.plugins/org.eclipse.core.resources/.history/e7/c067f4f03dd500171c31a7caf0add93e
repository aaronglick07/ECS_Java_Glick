
public class YourInitials 
{
	public static void main(String[] args) 
	{
		System.out.println("  A  ");
		System.out.println(" A  A");
		System.out.println("A    A");
		System.out.println("AAAAAA");
		System.out.println("A    A");
		System.out.println("A    A");
		System.out.println("A    A");
		
	}

}
